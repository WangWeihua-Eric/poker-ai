    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@midwayjs/cli": "^1.0.0",
    "@midwayjs/egg-ts-helper": "^1.0.1",
    "@midwayjs/luckyeye": "^1.0.0",
    "@midwayjs/mock": "^2.3.0",
    "@types/jest": "^26.0.10",
    "@types/mongoose": "~5.10.3",
    "@types/node": "14",
    "cross-env": "^6.0.0",
    "jest": "^26.4.0",
    "mwts": "^1.0.5",
    "ts-jest": "^26.2.0",
    "typescript": "^4.0.0"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "start:dev": "egg-scripts start --title=my-midway-project --framework=@midwayjs/web --env=dev",
    "start:testk8s": "egg-scripts start --title=my-midway-project --framework=@midwayjs/web --env=testk8s",
    "start:prodk8s": "egg-scripts start --title=my-midway-project --framework=@midwayjs/web --env=prodk8s",
    "stop": "egg-scripts stop --title=my-midway-project",
    "start_build": "npm run build && cross-env NODE_ENV=development midway-bin dev",
    "dev": "cross-env ets && cross-env MIDWAY_SERVER_ENV=dev midway-bin dev --ts",
    "test": "midway-bin test",
    "cov": "midway-bin cov",
    "lint": "mwts check",
    "lint:fix": "mwts fix",
    "ci": "npm run cov",
    "build": "midway-bin build -c",
    "check": "luckyeye"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ],
  "midway-luckyeye": {
    "packages": [
      "midway_v2"
    ]
  },
