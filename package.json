    "axios": "^0.21.1",
    "cos-nodejs-sdk-v5": "^2.9.12",
    "crypto": "^1.0.1",
    "egg": "^2.0.0",
    "egg-cors": "^2.2.3",
    "egg-scripts": "^2.10.0",
    "mongoose": "5.10.18",
    "querystring": "^0.2.1",
